{
  "name": "css-framework",
  "version": "1.0.0",
  "description": "A minimalistic, opinionated utility first css framework",
  "main": "dist/framework.css",
  "scripts": {
    "build": "sass src/framework.scss dist/framework.css",
    "watch": "sass --watch src:dist",
    "build:min": "sass --style=compressed src/framework.scss dist/framework.min.css",
    "dev": "npm run watch"
  },
